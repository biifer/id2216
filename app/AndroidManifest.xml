<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.example.com"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <application>
        <uses-library android:name="com.google.android.maps"/>
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:label="@string/normal_mode"
            android:name=".AppActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PreviewActivity" android:label="Preview"></activity>
        <activity android:name=".MainActivity" android:label="Main"></activity>
        <activity android:name=".OptionsActivity" android:label="Options"></activity>
            
        
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION">
        
    </uses-permission>	
    <uses-permission android:name="android.permission.INTERNET">
        
    </uses-permission>

     
</manifest>